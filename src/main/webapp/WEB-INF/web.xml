<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <servlet>
    <servlet-name>HelloServlet</servlet-name>
    <servlet-class>com.revature.controllers.HelloServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TestServlet</servlet-name>
    <servlet-class>com.revature.controllers.TestServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.revature.controllers.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SessionServlet</servlet-name>
    <servlet-class>com.revature.controllers.SessionTracking</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>WelcomeServlet</servlet-name>
    <servlet-class>com.revature.controllers.WelcomeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EmployeeReimbursementServlet</servlet-name>
    <servlet-class>com.revature.controllers.EmployeeReimbursementServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AssignedReimbursements</servlet-name>
    <servlet-class>com.revature.controllers.AssignedReimbursementServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ApproveReimbursementServlet</servlet-name>
    <servlet-class>com.revature.controllers.ApproveReimbursementServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TestServlet</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SessionServlet</servlet-name>
    <url-pattern>/session</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>WelcomeServlet</servlet-name>
    <url-pattern>/welcome</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EmployeeReimbursementServlet</servlet-name>
    <url-pattern>/reimbursements</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SubmissionFormServlet</servlet-name>
    <servlet-class>com.revature.controllers.SubmissionFormServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SubmissionFormServlet</servlet-name>
    <url-pattern>/submissionForm</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AssignedReimbursements</servlet-name>
    <url-pattern>/assignedReimbursements</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ApproveReimbursementServlet</servlet-name>
    <url-pattern>/approveReimbursement</url-pattern>
  </servlet-mapping>
  <display-name>Archetype Created Web Application</display-name>
</web-app>
